{
  "name": "quickly-desktop",
  "version": "1.9.5",
  "description": "QDesktop",
  "homepage": "https://quickly.com.tr",
  "author": {
    "name": "Kosmos Labs",
    "email": "info@kosmos.ist"
  },
  "keywords": [],
  "main": "main.js",
  "private": true,
  "overrides": {
    "node-sass": "npm:sass@^1.83.1",
    "@types/ms": "0.7.31",
    "@types/pouchdb-core": "6.1.3",
    "@types/pouchdb-find": "6.3.4",
    "@types/pouchdb-mapreduce": "6.1.3",
    "@types/pouchdb-replication": "6.4.2",
    "@types/express-serve-static-core": "4.17.0"
  },
  "scripts": {
    "ng": "ng",
    "index": "node tools/indexer/index.js",
    "lint": "ng lint",
    "start": "npm-run-all --parallel webpack:watch electron:serve",
    "webpack:watch": "webpack --watch",
    "start:web": "webpack-dev-server --content-base . --port 4200 --inline",
    "build:electron:main": "tsc -p tsconfig.electron.json && copyfiles package.json dist && cd dist && npm install --prod && cd ..",
    "build": "npx webpack --display-error-details && npm run build:electron:main",
    "build:prod": "cross-env NODE_ENV=production npm run build",
    "electron:serve": "npm run build:electron:main && electron ./dist --serve",
    "electron:test": "electron ./dist",
    "electron:dev": "npm run build && electron ./dist",
    "electron:prod": "npm run build:prod && electron ./dist",
    "electron:linux": "npm run build:prod && npx electron-builder build --linux",
    "electron:linux32": "npm run build:prod && npx electron-builder build --linux --ia32",
    "electron:windows": "npm run build:prod && npx electron-builder build --windows",
    "electron:mac": "npm run build:prod && npx electron-builder build --mac",
    "release": "rm -rf app-builds && run-s -c electron:linux package",
    "package": "cd app-builds && mv linux-unpacked App &&  zip -r App.zip App",
    "test": "cross-env node tools/agent/karma-run.js -- --single-run --browsers ChromeHeadlessNoSandbox",
    "test:trace": "cross-env NODE_OPTIONS=\"--require ./tools/agent/fs-trace-eisdir.js\" node tools/agent/karma-run.js -- --single-run --browsers ChromeHeadlessNoSandbox",
    "test:debug": "cross-env node tools/agent/karma-run.js -- --single-run --browsers ChromeHeadlessNoSandbox --log-level debug",
    "test:compile": "cross-env node tools/agent/karma-run.js --compile-only",
    "docs:verify": "bash ops/scripts/verify-docs.sh",
    "docs:repo-map": "bash ops/scripts/gen-repo-map.sh",
    "docs:symbol-index": "bash ops/scripts/gen-symbol-index.sh",
    "verify": "node tools/agent/verify.js",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet && npm run build",
    "e2e": "protractor ./protractor.conf.js",
    "compodoc": "./node_modules/.bin/compodoc -p ./tsconfig.json",
    "upload": "scp -r ./app-builds/App.zip root@quickly.com.tr:/var/www/cdn.quickly.com.tr/app",
    "publish": "npm-run-all -s -c release package upload"
  },
  "dependencies": {
    "@angular/common": "5.0.3",
    "@angular/compiler": "5.0.3",
    "@angular/core": "5.0.3",
    "@angular/forms": "5.0.3",
